.key nope
set prefix locknchase
set game ${prefix}_000
copy $game.RNC DF0:$game
protect DF0:$game RWED
set game ${prefix}_020
copy $game.RNC DF0:$game
protect DF0:$game RWED
copy readme.md df0:

; copy startup exe
set game ${prefix}
copy $game DF0:$game

if exists df0:$game.high
   delete df0:$game.high
endif

set arc ${game}_HD.lha
if exists ${arc}
    echo deleting ${arc}...
    delete ${arc}
endif

; also archive with lha
lha a -r $arc ${game}_HD

